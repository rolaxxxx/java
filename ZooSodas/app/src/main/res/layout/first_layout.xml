<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible">

    <Button
        android:id="@+id/Idarbinti_Darbuotoja"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="136dp"
        android:layout_marginLeft="136dp"
        android:layout_marginTop="301dp"
        android:text="Idarbinti" />

    <EditText
        android:id="@+id/DarbuotojoVardas"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Ivesti darbuotojo varda"
        android:textAppearance="@android:style/TextAppearance.Holo.Medium" />

    <EditText
        android:id="@+id/DarbuotojoPavarde"
        android:layout_width="470dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/DarbuotojoVardas"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="95dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Ivesti darbuotojo pavarde"
        android:textAppearance="@android:style/TextAppearance.Holo.Medium"
        android:layout_marginLeft="0dp"
        android:layout_alignLeft="@+id/DarbuotojoVardas" />

    <Spinner
        android:id="@+id/GyvunuKategorijosDarbuotojuKlaseje"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:layout_alignParentStart="true"

        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="227dp"
        android:padding="8dp"
        android:text="Gyvunai"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="159dp"
        android:text="Pasirinkite gyvuno kategorija is saraso "
        android:textAppearance="@android:style/TextAppearance.Holo.Medium"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="0dp" />


</RelativeLayout>